{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:clean": "npm run kill-port && npm run dev",
    "dev:smart": "./start-dev.sh",
    "dev:https": "next dev -p 3000 --experimental-https --experimental-https-key ./certificates/localhost.key --experimental-https-cert ./certificates/localhost.crt",
    "kill-port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
    "fresh-start": "rm -rf .next && npm run kill-port && npm run build && npm run start",
    "clean-restart": "./optimal-start.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "node scripts/test-runner.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='**/*.{js,jsx,ts,tsx}' --coverageDirectory=coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=html",
    "test:ci": "node scripts/test-runner.js --fail-fast",
    "test:unit": "jest --selectProjects unit --testPathPattern=__tests__/unit",
    "test:integration": "jest --selectProjects integration --testPathPattern=__tests__/integration",
    "test:e2e": "jest --selectProjects e2e --testPathPattern=__tests__/e2e",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:performance": "jest --selectProjects performance --testPathPattern=__tests__/performance",
    "test:unit:watch": "jest --selectProjects unit --testPathPattern=__tests__/unit --watch",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:quick": "npm run test:unit && npm run lint && npm run type-check",
    "test:all": "node scripts/test-runner.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:clear-cache": "jest --clearCache",
    "test:coverage:open": "npm run test:coverage && open coverage/lcov-report/index.html",
    "test:performance:ci": "jest --selectProjects performance --ci --silent",
    "test:smoke": "jest __tests__/smoke --passWithNoTests",
    "test:voice": "node scripts/test-voice-ordering.cjs",
    "test:voice:sequential": "node scripts/test-voice-ordering.cjs --sequential",
    "test:voice:no-coverage": "node scripts/test-voice-ordering.cjs --no-coverage",
    "test:voice:quick": "node scripts/test-voice-ordering.cjs --no-coverage --sequential",
    "demo:voice": "node scripts/voice-optimization-demo.cjs",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "seed-demo": "tsx scripts/seed-demo-data.ts",
    "setup-guest": "tsx scripts/setup-guest-account.ts",
    "demo:setup": "npm run setup-guest && npm run seed-demo && npm run demo:check",
    "demo:reset-password": "tsx scripts/reset-guest-password.ts",
    "demo:cleanup": "tsx scripts/demo-data-reset.ts",
    "supabase:migrate": "supabase db push",
    "vercel-build": "next build",
    "prepare": "husky",
    "demo:check": "tsx scripts/demo-readiness-check.ts",
    "demo:quick-reset": "tsx scripts/demo-data-reset.ts quick",
    "demo:full-reset": "tsx scripts/demo-data-reset.ts full",
    "analyze": "ANALYZE=true npm run build",
    "perf:build": "npm run build && npm run analyze",
    "migrate:perf": "supabase db push --db-url postgresql://postgres:postgres@127.0.0.1:54322/postgres",
    "demo:complete-reset": "tsx scripts/demo-data-reset.ts complete",
    "demo:backup": "tsx scripts/demo-data-reset.ts backup",
    "demo:list-backups": "tsx scripts/demo-data-reset.ts list-backups",
    "demo:health-check": "tsx scripts/demo-readiness-check.ts",
    "demo:emergency-reset": "npm run demo:complete-reset && npm run demo:setup",
    "demo:verify": "npm run demo:check && echo 'Demo system ready!'",
    "demo:metrics": "curl -s http://localhost:3000/api/demo/health-check | jq .",
    "demo:logs": "echo 'Demo logs - check console and network tab'",
    "db:perf-test": "tsx scripts/apply-migration-simple.ts",
    "db:check-schema": "tsx scripts/check-schema.ts",
    "db:migration-guide": "tsx scripts/apply-sql-direct.ts",
    "monitor:health": "tsx scripts/health-monitor.ts",
    "monitor:health:continuous": "tsx scripts/health-monitor.ts --continuous",
    "monitor:production": "tsx scripts/health-monitor.ts --url https://myapp.vercel.app --continuous",
    "validate:production": "tsx scripts/production-readiness.ts",
    "validate:deployment": "npm run test:deployment && npm run validate:production",
    "test:deployment": "jest __tests__/deployment --passWithNoTests",
    "monitor:setup": "echo 'Setting up monitoring...' && npm run supabase:migrate",
    "monitor:verify": "tsx scripts/verify-monitoring.ts",
    "monitor:verify:production": "tsx scripts/verify-monitoring.ts https://your-app.vercel.app"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@next/bundle-analyzer": "^15.3.3",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "latest",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "latest",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "latest",
    "@radix-ui/react-scroll-area": "latest",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-separator": "latest",
    "@radix-ui/react-slider": "latest",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "latest",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toast": "latest",
    "@radix-ui/react-tooltip": "latest",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/dompurify": "^3.0.5",
    "@types/pg": "^8.15.4",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.5.0",
    "isomorphic-dompurify": "^2.25.0",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "next-themes": "^0.4.4",
    "openai": "^4.102.0",
    "pg": "^8.16.0",
    "puppeteer": "^24.10.0",
    "react": "^19",
    "react-day-picker": "9.6.7",
    "react-dom": "^19",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.54.1",
    "react-resizable-panels": "^2.1.7",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.4.0-canary.51",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^16.1.0",
    "node-fetch": "^3.3.2",
    "node-mocks-http": "^1.15.1",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
